{
    "manifest_version" : 2,
    "name": "Healthy Break Extension",
    "version": "1",
    "content_scripts": [
     {
         "matches": [
             "<all_urls>"
         ],
         "js": ["content.js"]
     }
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "<all_urls>"],
    
    "browser_action": {
        "default_icon": "corgi.png",
        "default_popup": "index.html",
        "default_title": "Healthy Breaks"
    }



}